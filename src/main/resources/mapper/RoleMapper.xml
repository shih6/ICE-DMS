        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shih.icecms.mapper.RoleMapper">

    <resultMap id="BaseResultMap" type="com.shih.icecms.entity.Role">
            <id property="id" column="id" jdbcType="INTEGER"/>
